query{
  repository(owner:"nkalexiou", name:"suricatajs"){
    name
    description
    vulnerabilityAlerts(first:30){
      totalCount
      edges{
        node{
          securityVulnerability{
            package{
              name
              ecosystem
            }
            severity
            vulnerableVersionRange
          }
        }
      }
    }
    
    languages(first:4) {
      edges{
        node{
          name
        }
      }
    }
  }
}
